<?xml version="1.0" encoding="UTF-8"?>
<extension id="page_prototypes" status="released" xmlns="http://symphony-cms.com/schemas/extension/1.0">
	<name>Page Prototypes</name>
	<description>Allows pages to act as prototypes for other pages.</description>
	<repo type="github">https://github.com/klaftertief/page_prototypes</repo>
	<url type="discuss">http://symphony-cms.com/discuss/thread/48137/</url>
	<url type="issues">https://github.com/klaftertief/page_prototypes/issues</url>
	<types>
		<type>Workflow</type>
	</types>
	<authors>
		<author>
			<name github="klaftertief" symphony="klaftertief" twitter="klaftertief">Jonas Coch</name>
			<website>http://klaftertief.de</website>
			<email>jonas@klaftertief.de</email>
		</author>
	</authors>
	<releases>
		<release version="1.0beta" date="2012-05-14" min="2.3">
			* Complete rewrite, prototypes are now normal pages with type `prototype`
		</release>
		<release version="0.6.1" date="2012-02-10" min="2.2">
			* Added `extension.meta.xml` file
			* Update referenced pages when saving a prototype
		</release>
		<release version="0.6" date="2011-02-09" min="2.2"/>
	</releases>
</extension>